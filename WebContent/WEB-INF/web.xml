<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
	<display-name>ToniHoliday</display-name>
  	<welcome-file-list>
    	<welcome-file>index.jsp</welcome-file>
  	</welcome-file-list>
  	<servlet>
		<servlet-name>NoSpecificDate</servlet-name>
		<servlet-class>flights.eu.toniholiday.NoSpecificDate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NoSpecificDate</servlet-name>
		<url-pattern>/noDate.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SpecificDate</servlet-name>
		<servlet-class>flights.eu.toniholiday.SpecificDate</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SpecificDate</servlet-name>
		<url-pattern>/specificDate.do</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MySQLJDBCRegister</servlet-name>
		<servlet-class>tomcatInit.JDBCDriverRegister</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>AJAX</servlet-name>
		<servlet-class>flights.eu.toniholiday.GetDataByAJAX</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AJAX</servlet-name>
		<url-pattern>/getDataAjax.do</url-pattern>
	</servlet-mapping>
	<error-page>
		<exception-type>java.lang.NullPointerException</exception-type>
		<location>/errorPage.jsp</location>
	</error-page>
</web-app>